{
    "name": "themes-starter",
    "version": "3.0.0",
    "license": "GPL-2.0",
    "description": "A WordPress Starter Theme built with Bootstrap 5.",
    "homepage": "https://them.es",
    "author": "them.es",
    "repository": {
        "type": "git",
        "url": "git://github.com/them-es/themes-starter.git"
    },
    "type": "module",
    "scripts": {
        "watch": "webpack --watch --config webpack.config.js --mode development && wp-scripts start",
        "build": "webpack --config webpack.config.js --mode production && wp-scripts build"
    },
    "browserslist": [
        "last 2 versions"
    ],
    "dependencies": {
        "@popperjs/core": "^2.11.8",
        "bootstrap": "^5.3.1",
        "bootstrap-icons": "^1.11.3",
        "jquery": "^3.7.0"
    },
    "devDependencies": {
        "@wordpress/scripts": "^18.1.0",
        "css-loader": "^7.1.2",
        "mini-css-extract-plugin": "^2.9.0",
        "postcss-loader": "^8.1.1",
        "sass": "^1.77.8",
        "sass-loader": "^16.0.0",
        "style-loader": "^3.3.4",
        "webpack": "^5.93.0",
        "webpack-cli": "^5.1.4",
        "webpack-remove-empty-scripts": "^1.0.4"
    }
}
